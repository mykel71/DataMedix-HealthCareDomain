{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-datamedix"
		},
		"AzureDataLakeStorage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage'"
		},
		"AzureDatabricksDeltaLake_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'AzureDatabricksDeltaLake'"
		},
		"ls_azuresqldb_jhb_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_azuresqldb_jhb'"
		},
		"ls_azuresqldb_tshwane_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_azuresqldb_tshwane'"
		},
		"AzureDataLakeStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storaccdatamedix.dfs.core.windows.net/"
		},
		"ls_azuresqldb_jhb_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "datamedixserverdev.database.windows.net"
		},
		"ls_azuresqldb_jhb_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "JohannesburgMedSolutions"
		},
		"ls_azuresqldb_jhb_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "mikedev"
		},
		"ls_azuresqldb_tshwane_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "datamedixserverdev.database.windows.net"
		},
		"ls_azuresqldb_tshwane_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "TshwaneMedSolutions"
		},
		"ls_azuresqldb_tshwane_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "mikedev"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/AzureDataLakeStorage')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobFS",
				"typeProperties": {
					"url": "[parameters('AzureDataLakeStorage_properties_typeProperties_url')]",
					"accountKey": {
						"type": "SecureString",
						"value": "[parameters('AzureDataLakeStorage_accountKey')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/AzureDatabricksDeltaLake')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureDatabricksDeltaLake",
				"typeProperties": {
					"domain": "https://adb-1384142462849466.6.azuredatabricks.net",
					"clusterId": "0217-100642-tuxi6xuz",
					"accessToken": {
						"type": "SecureString",
						"value": "[parameters('AzureDatabricksDeltaLake_accessToken')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/ls_azuresqldb_jhb')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureSqlDatabase",
				"typeProperties": {
					"server": "[parameters('ls_azuresqldb_jhb_properties_typeProperties_server')]",
					"database": "[parameters('ls_azuresqldb_jhb_properties_typeProperties_database')]",
					"encrypt": "mandatory",
					"trustServerCertificate": false,
					"authenticationType": "SQL",
					"userName": "[parameters('ls_azuresqldb_jhb_properties_typeProperties_userName')]",
					"password": {
						"type": "SecureString",
						"value": "[parameters('ls_azuresqldb_jhb_password')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/ls_azuresqldb_tshwane')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureSqlDatabase",
				"typeProperties": {
					"server": "[parameters('ls_azuresqldb_tshwane_properties_typeProperties_server')]",
					"database": "[parameters('ls_azuresqldb_tshwane_properties_typeProperties_database')]",
					"encrypt": "mandatory",
					"trustServerCertificate": false,
					"authenticationType": "SQL",
					"userName": "[parameters('ls_azuresqldb_tshwane_properties_typeProperties_userName')]",
					"password": {
						"type": "SecureString",
						"value": "[parameters('ls_azuresqldb_tshwane_password')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}
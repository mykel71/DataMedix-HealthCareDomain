{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-datamedix"
		},
		"AzureDataLakeStorage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage'"
		},
		"AzureDatabricksDeltaLake_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'AzureDatabricksDeltaLake'"
		},
		"ls_azuresqldb_jhb_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_azuresqldb_jhb'"
		},
		"ls_azuresqldb_tshwane_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_azuresqldb_tshwane'"
		},
		"AzureDataLakeStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storaccdatamedix.dfs.core.windows.net/"
		},
		"ls_azuresqldb_jhb_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "datamedixserverdev.database.windows.net"
		},
		"ls_azuresqldb_jhb_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "JohannesburgMedSolutions"
		},
		"ls_azuresqldb_jhb_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "mikedev"
		},
		"ls_azuresqldb_tshwane_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "datamedixserverdev.database.windows.net"
		},
		"ls_azuresqldb_tshwane_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "TshwaneMedSolutions"
		},
		"ls_azuresqldb_tshwane_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "mikedev"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-datamedix_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage_accountKey": {
						"value": "[parameters('AzureDataLakeStorage_accountKey')]"
					},
					"AzureDatabricksDeltaLake_accessToken": {
						"value": "[parameters('AzureDatabricksDeltaLake_accessToken')]"
					},
					"ls_azuresqldb_jhb_password": {
						"value": "[parameters('ls_azuresqldb_jhb_password')]"
					},
					"ls_azuresqldb_tshwane_password": {
						"value": "[parameters('ls_azuresqldb_tshwane_password')]"
					},
					"AzureDataLakeStorage_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage_properties_typeProperties_url')]"
					},
					"ls_azuresqldb_jhb_properties_typeProperties_server": {
						"value": "[parameters('ls_azuresqldb_jhb_properties_typeProperties_server')]"
					},
					"ls_azuresqldb_jhb_properties_typeProperties_database": {
						"value": "[parameters('ls_azuresqldb_jhb_properties_typeProperties_database')]"
					},
					"ls_azuresqldb_jhb_properties_typeProperties_userName": {
						"value": "[parameters('ls_azuresqldb_jhb_properties_typeProperties_userName')]"
					},
					"ls_azuresqldb_tshwane_properties_typeProperties_server": {
						"value": "[parameters('ls_azuresqldb_tshwane_properties_typeProperties_server')]"
					},
					"ls_azuresqldb_tshwane_properties_typeProperties_database": {
						"value": "[parameters('ls_azuresqldb_tshwane_properties_typeProperties_database')]"
					},
					"ls_azuresqldb_tshwane_properties_typeProperties_userName": {
						"value": "[parameters('ls_azuresqldb_tshwane_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}